{
  "name": "vue-form-components",
  "version": "1.1.13",
  "description": "A collection of bootstrap form components for Vue 2",
  "main": "dist/build.js",
  "scripts": {
    "test": "jest --coverage",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/georgehanson/Vue-Form-Components.git"
  },
  "keywords": [
    "vue2",
    "vue",
    "form",
    "components",
    "bootstrap"
  ],
  "author": "George Hanson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/georgehanson/Vue-Form-Components/issues"
  },
  "homepage": "https://github.com/georgehanson/Vue-Form-Components#readme",
  "dependencies": {
    "vue": "^2.2.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "snapshotSerializers": [
      "<rootDir>/jest/htmlSnapshotBeautifier.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.0.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "jest": "^19.0.2",
    "jest-vue-preprocessor": "^0.1.3",
    "js-beautify": "^1.6.11",
    "node-sass": "^4.5.0",
    "sass-loader": "^5.0.1",
    "vue": "^2.2.1",
    "vue-loader": "^11.1.4",
    "vue-server-renderer": "^2.2.1",
    "vue-template-compiler": "^2.2.1",
    "vue-template-es2015-compiler": "^1.5.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
